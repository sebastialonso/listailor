Ingresa con el proveedor que deseas.
<br />
<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
    <% if provider == :google_oauth2 %>
    <br />
      <%= link_to "Sign in with Google+", omniauth_authorize_path(resource_name, provider), :class => "btn btn-danger btn-large" %><br />
    <% end %>
    
  <% end -%>
  <br />
  <%= link_to "Volver", :root, :class => "btn btn-lg btn-inverse" %>
<% end -%>
